{
  "name": "personal_site",
  "version": "1.0.0",
  "description": "Personal portfolio website showcasing software engineering projects, blog, and bookshelf",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development --open",
    "server": "cd server && npm run dev",
    "server:build": "cd server && npm run build",
    "server:start": "cd server && npm start",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "install:all": "npm install && cd server && npm install",
    "build:all": "npm run build && npm run server:build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md}'",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/korykilpatrick/personal_site.git"
  },
  "keywords": [
    "portfolio",
    "personal-website",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/korykilpatrick/personal_site/issues"
  },
  "homepage": "https://github.com/korykilpatrick/personal_site#readme",
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.2.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "axios": "^1.6.8",
    "express": "^4.19.2",
    "fuse.js": "^7.0.0",
    "markdown-to-jsx": "^7.4.1",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
